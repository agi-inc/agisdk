    {
      "description": "Correct destination address (1030 Post St)",
      "type": "jmespath",
      "expected_value": "1030 Post Street Apartments",
      "query": "currentTrips[0].destination.name"
    },
    {
      "description": "Car can fit 5+ people (UdriverXL with 6 seats)",
      "type": "jmespath",
      "expected_value": true,
      "query": "currentTrips[0].car.seats >= `5`"
    },
    {
      "description": "UdriverXL car type was selected",
      "type": "jmespath",
      "expected_value": "UdriverXL",
      "query": "currentTrips[0].car.type"
    },
    {
      "description": "Trip price is $25.41",
      "type": "jmespath",
      "expected_value": 25.41,
      "query": "currentTrips[0].car.finalPrice"
    }
