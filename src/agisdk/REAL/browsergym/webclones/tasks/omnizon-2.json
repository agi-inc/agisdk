{
  "id": "omnizon-2",
  "goal": "Buy a Ninja Air Fryer, and then after buying it. Cancel the order because you bought it by mistake.",
  "website": {
    "id": "omnizon",
    "name": "Omnizon",
    "similarTo": "Amazon",
    "previewImage": "/websitePreviews/omnizon_preview.jpg",
    "url": "https://real-omnizon.vercel.app/"
  },
  "difficulty": "medium",
  "challengeType": "action",
  "possible": true,
  "evals": [
    {
      "description": "Order was cancelled successfully",
      "type": "jmespath",
      "expected_value": true,
      "query": "length(order.cancelledOrders) > `0`"
    },
    {
      "description": "Cancellation reason indicates it was ordered by mistake",
      "type": "jmespath",
      "expected_value": "Ordered by mistake",
      "query": "order.cancelledOrders[0].cancellationReason"
    },
    {
      "description": "Cancelled item was a Ninja Air Fryer (product ID 2)",
      "type": "jmespath",
      "expected_value": 2,
      "query": "order.cancelledOrders[0].items[0].id"
    },
    {
      "description": "No active orders remain after cancellation",
      "type": "jmespath",
      "expected_value": 0,
      "query": "length(order.orders)"
    },
    {
      "description": "Cancellation number was generated",
      "type": "jmespath",
      "expected_value": true,
      "query": "starts_with(order.cancelledOrders[0].cancellationNumber, 'CAN-')"
    }
  ],
  "points": 1,
  "config": {}
}