  "evals": [
    {
      "description": "Reply recipient is Jane Smith",
      "type": "jmespath",
      "expected_value": "jane.smith@example.com",
      "query": "goMail_mails.emails[?isReply==`true`] | [0].to[0]"
    },
    {
      "description": "Reply content contains OOO message",
      "type": "jmespath",
      "expected_value": "OOO",
      "query": "goMail_mails.emails[?isReply==`true`] | [0].content"
    },
    {
      "description": "Reply is marked as sent",
      "type": "jmespath",
      "expected_value": true,
      "query": "goMail_mails.emails[?isReply==`true`] | [0].sent"
    },
    {
      "description": "Reply has correct thread ID",
      "type": "jmespath",
      "expected_value": "thread_2",
      "query": "goMail_mails.emails[?isReply==`true`] | [0].threadId"
    }
  ],
  "points": 1,
