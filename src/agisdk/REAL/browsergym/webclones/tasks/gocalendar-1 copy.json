{
  "id": "gocalendar-1",
  "goal": "Create an event on wednesday called Sync with Emily, add a Google Meet and make sure Emily is added. In the description add Marketing updates and set the notifications for 5 minutes before.",
  "website": {
    "id": "gocalendar",
    "name": "GoCalendar",
    "similarTo": "Google Calendar",
    "previewImage": "/websitePreviews/gocalendar_preview.jpg",
    "url": "https://real-gocalendar.vercel.app/"
  },
  "difficulty": "easy",
  "challengeType": "action",
  "possible": true,
  "evals": [
    {
      "description": "Check if a new event was added with title 'Sync with emily'",
      "type": "jmespath",
      "expected_value": 1,
      "query": "state.calendar.myEvents[?title=='Sync with emily'] | length(@)"
    },
    {
      "description": "Check events on July 24, 2024",
      "type": "jmespath",
      "expected_value": 1,
      "query": "state.calendar.myEvents[?contains(start, '2024-07-24')] | length(@)"
    },
    {
      "description": "Verify no tasks were added (task count remains 0)",
      "type": "jmespath",
      "expected_value": 0,
      "query": "state.tasks.myTasks || `[]` | length(@)"
    },
    {
      "description": "Check Work calendar events count",
      "type": "jmespath",
      "expected_value": 22,
      "query": "state.calendar.myEvents[?calendarOptions.calendar.id=='1'] | length(@)"
    }
  ],
  "points": 1,
  "config": {}
}