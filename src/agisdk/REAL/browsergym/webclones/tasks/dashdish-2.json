{
  "id": "dashdish-2",
  "goal": "Get Express Delivery for an order from Ike's Sandwiches of regular size 'The Godfather Sandwich' and make sure there are no onions on it.",
  "website": {
    "id": "dashdish",
    "name": "DashDish",
    "similarTo": "Doordash",
    "previewImage": "/websitePreviews/dashdish_preview.jpg",
    "url": "https://real-dashdish.vercel.app/"
  },
  "difficulty": "medium",
  "challengeType": "action",
  "possible": true,
  "evals": [
    {
      "description": "Order contains item from Ike's Sandwiches",
      "type": "jmespath",
      "expected_value": "Ike's Love & Sandwiches",
      "query": "cart.foodOrders[0].cartItems[0].restaurantName"
    },
    {
      "description": "Order contains The Godfather Sandwich",
      "type": "jmespath",
      "expected_value": "The Godfather Sandwich",
      "query": "cart.foodOrders[0].cartItems[0].name"
    },
    {
      "description": "Sandwich size is Regular",
      "type": "jmespath",
      "expected_value": "Regular",
      "query": "cart.foodOrders[0].cartItems[0].size"
    },
    {
      "description": "Express delivery option selected",
      "type": "jmespath",
      "expected_value": "Express",
      "query": "cart.foodOrders[0].checkoutDetails.shipping.deliveryOption"
    },
    {
      "description": "No onions removal option selected",
      "type": "jmespath",
      "expected_value": true,
      "query": "contains(cart.foodOrders[0].cartItems[0].removalOptions, 'no_onions')"
    }
  ],
  "points": 1,
  "config": {}
}