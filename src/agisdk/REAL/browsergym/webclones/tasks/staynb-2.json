{
  "id": "staynb-2",
  "goal": "Search for the best rated place to stay in Boston, which has a Balcony. Book it for 2 adults, pay part of it now and pay later with gpay.",
  "website": {
    "id": "staynb",
    "name": "StayNB",
    "similarTo": "Airbnb",
    "previewImage": "/websitePreviews/staynb_preview.jpg",
    "url": "https://real-staynb.vercel.app/"
  },
  "difficulty": "medium",
  "challengeType": "action",
  "possible": true,
  "evals": [
    {
      "description": "Booking was completed successfully",
      "type": "jmespath",
      "expected_value": true,
      "query": "length(booking.bookingDetails) > `0`"
    },
    {
      "description": "Guest count is set to 2 adults",
      "type": "jmespath",
      "expected_value": 2,
      "query": "booking.bookingDetails[0].guests.Adults"
    },
    {
      "description": "No children were included in the booking",
      "type": "jmespath",
      "expected_value": 0,
      "query": "booking.bookingDetails[0].guests.Children"
    },
    {
      "description": "Payment method is set to Google Pay",
      "type": "jmespath",
      "expected_value": "Google Pay",
      "query": "booking.bookingDetails[0].payWith"
    },
    {
      "description": "Pay later option was selected",
      "type": "jmespath",
      "expected_value": "pay-later",
      "query": "booking.bookingDetails[0].pay_when"
    },
    {
      "description": "Booked property is located in Boston",
      "type": "jmespath",
      "expected_value": "Boston",
      "query": "booking.bookingDetails[0].stay.city"
    },
    {
      "description": "A reservation ID was generated",
      "type": "jmespath",
      "expected_value": true,
      "query": "booking.bookingDetails[0].reservationId != null"
    }
  ],
  "points": 1,
  "config": {}
}